{
  "name": "javascript-status",
  "version": "1.3.0",
  "main": "build/index.js",
  "description": "Javascript Status is tiny HTTP status code utility library for readability.",
  "scripts": {
    "test": "jest",
    "build": "npm run clean && babel src --out-dir build",
    "clean": "rm -rf build",
    "lint": "eslint src test",
    "check": "./node_modules/.bin/npm-check -u",
    "prepare": "npm run clean && npm run lint && npm run test && npm run build",
    "pub": "./node_modules/.bin/np || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaibhavmule/javscript-status.git"
  },
  "keywords": [
    "http",
    "status",
    "status-code",
    "http-status-code",
    "javscript-status"
  ],
  "author": "Vaibhav Mule <vaibhavmule135@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vaibhavmule/javscript-status/issues"
  },
  "homepage": "https://github.com/vaibhavmule/javscript-status#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "jest": "^21.2.1",
    "np": "^2.17.0",
    "npm-check": "^5.5.2"
  }
